{
  "name": "@project89/argos-sdk",
  "version": "0.1.48",
  "description": "SDK for Project89 Argos",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./client/react": {
      "types": "./dist/types/client/react/index.d.ts",
      "import": "./dist/client/react/index.mjs",
      "require": "./dist/client/react/index.js",
      "default": "./dist/client/react/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist .build-temp",
    "build:types": "tsc -p tsconfig.declarations.json",
    "build:client": "tsc -p tsconfig.client.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:bundle": "rollup -c",
    "build": "npm run clean && npm run build:types && npm run build:client && npm run build:server && npm run build:bundle",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.5.1",
    "js-cookie": "^3.0.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-fetch": "^2.7.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^2.79.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "analytics",
    "tracking",
    "fingerprinting",
    "presence",
    "visits",
    "react",
    "typescript",
    "server",
    "browser"
  ],
  "author": "Project 89",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneirocom/argos-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/oneirocom/argos-sdk/issues"
  },
  "homepage": "https://github.com/oneirocom/argos-sdk#readme"
}
