{
  "name": "@project89/argos-sdk",
  "version": "0.1.4",
  "private": false,
  "description": "üîç Argos SDK - A toolkit for creating, managing, and interacting with distributed narratives in modern web applications. Built with TypeScript and React",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/lib/argos-sdk/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/lib/argos-sdk/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c rollup.config.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --options typedoc.json",
    "docs:watch": "typedoc --options typedoc.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-89/argos-sdk.git"
  },
  "keywords": [
    "analytics",
    "tracking",
    "fingerprinting",
    "presence",
    "user-tracking",
    "privacy",
    "web-analytics",
    "real-time",
    "web3",
    "blockchain",
    "dapp"
  ],
  "author": "Project89",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/project-89/argos-sdk/issues"
  },
  "homepage": "https://github.com/project-89/argos-sdk#readme",
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.4.5",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1"
  }
}
