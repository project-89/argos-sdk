{
  "name": "@project89/argos-sdk",
  "version": "0.1.37",
  "description": "SDK for Project89 Argos",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./server": {
      "require": "./dist/cjs/server/index.js",
      "import": "./dist/esm/server/index.js",
      "types": "./dist/types/server/index.d.ts"
    },
    "./react": {
      "require": "./dist/cjs/react/index.js",
      "import": "./dist/esm/react/index.js",
      "types": "./dist/types/react/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "tsc -p tsconfig.declarations.json",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:bundle": "rollup -c",
    "build": "npm run clean && npm run build:types && npm run build:ts && npm run build:bundle",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.5.1",
    "cross-fetch": "^3.1.5",
    "js-cookie": "^3.0.5",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-fetch": "^2.7.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "analytics",
    "tracking",
    "fingerprinting",
    "presence",
    "visits",
    "react",
    "typescript",
    "server",
    "browser"
  ],
  "author": "Project 89",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneirocom/argos-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/oneirocom/argos-sdk/issues"
  },
  "homepage": "https://github.com/oneirocom/argos-sdk#readme"
}
